{
  "name": "fastify-jwt-typeorm",
  "version": "1.0.0",
  "description": "nodejs typescript template",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "author": "Mahmoud Mraisi",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmraisi/fastify-jwt-typeorm"
  },
  "bugs": {
    "url": "https://github.com/mmraisi/fastify-jwt-typeorm/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node dist/src/index.js",
    "build": "rm -rf dist && tsc --build",
    "dev": "nodemon src/index.ts",
    "test:unit": "glob -c 'tsx --test' 'test/unit/**/*.test.ts'",
    "test:unit:100": "glob -c 'c8 --100 --reporter html --reporter text tsx --test' 'test/unit/**/*.test.ts'",
    "test:integration": "NODE_ENV=test glob -c 'tsx --test' 'test/integration/**/*.test.ts'",
    "test:ci": "npm run test:unit:100 && npm run test:integration"
  },
  "keywords": [
    "orm",
    "jwt",
    "fastify",
    "openAPI"
  ],
  "license": "MIT",
  "dependencies": {
    "@dnlup/fastify-traps": "^3.0.2",
    "@fastify/autoload": "^5.8.3",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "api-problem": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.0",
    "http-status-codes": "^2.3.0",
    "oas-fastify": "^3.1.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "glob": "^10.4.1",
    "nodemon": "^3.1.3",
    "semantic-release": "^23.1.1",
    "sinon": "^17.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.2",
    "typescript": "^5.4.5"
  }
}
